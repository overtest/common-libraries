<Project Sdk="Microsoft.NET.Sdk">

    <PropertyGroup>
        <TargetFramework>net8.0</TargetFramework>
        <ImplicitUsings>enable</ImplicitUsings>
        <Nullable>disable</Nullable>
        <RootNamespace>Sirkadirov.Overtest.Common.TaskModels</RootNamespace>
        <LangVersion>latest</LangVersion>
        <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
        <Title>Overtest Task Models</Title>
        <Authors>Yurii Kadirov (aka Sirkadirov)</Authors>
        <PackageProjectUrl>https://github.com/overtest/common-libraries</PackageProjectUrl>
        <PackageLicenseUrl>https://github.com/overtest/common-libraries/blob/main/LICENSE.MD</PackageLicenseUrl>
        <RepositoryUrl>https://github.com/overtest/common-libraries</RepositoryUrl>
        <RepositoryType>git</RepositoryType>
        <PackageRequireLicenseAcceptance>true</PackageRequireLicenseAcceptance>
        <Copyright>Copyright (C) 2023 Yurii Kadirov (aka Sirkadirov)</Copyright>
        <Product>Overtest Task Models</Product>
        <AssemblyVersion>0.2.0</AssemblyVersion>
        <FileVersion>0.2.0</FileVersion>
        <Version>0.2.0</Version>
        <Description>An internal Overtest library to serialize programming tasks from JSON and other formats using .NET built-in capabilities</Description>
        <PackageReleaseNotes>See commits history at https://github.com/overtest/common-libraries/commits/main/</PackageReleaseNotes>
        <PackageId>Sirkadirov.Overtest.Common.TaskModels</PackageId>
    </PropertyGroup>

</Project>
